{
  "name": "software-testing-assignment",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js && coveralls < coverage/lcov.info"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "coveralls": "^3.1.1",
    "cross-env": "^7.0.3",
    "jest": "^29.3.1",
    "jest-chain": "^1.1.6",
    "jest-extended": "^3.2.0"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverage": true,
    "coverageReporters": [
      "lcov"
    ],
    "collectCoverageFrom": [
      "src/**/*.js','!src/.internal/*.js"
    ]
  }
}
